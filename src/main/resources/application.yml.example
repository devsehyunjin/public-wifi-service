spring:
  mvc:
    view:
      prefix: /WEB-INF/views/
      suffix: .jsp

  datasource:
    url: jdbc:sqlite:./wifi.db
    driver-class-name: org.sqlite.JDBC

  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        dialect: org.hibernate.community.dialect.SQLiteDialect
        jdbc:
          batch_size: 500
        order_inserts: true
        order_updates: true

seoul:
  api-key: ${SEOUL_API_KEY:your_seoul_api_key_here}
  host: openapi.seoul.go.kr
  port: 8088
  dataset: TbPublicWifiInfo
